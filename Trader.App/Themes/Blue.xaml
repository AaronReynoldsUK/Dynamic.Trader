<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--ACCENT COLORS-->
    <Color x:Key="HighlightDarkColor">#FF086F9E</Color>
    <Color x:Key="HighlightLightColor">#FF086F9E</Color>

    <Color x:Key="AccentDarkColor">#FF0072C6</Color>
    
    <!--80%-->
    <Color x:Key="AccentColor">#FF0072C6</Color>
    <!--60%-->
    <Color x:Key="AccentColor2">#CC0072C6</Color>

    <!--60%-->
    <!--<Color x:Key="AccentColor2">MediumPurple</Color>-->
    <!--40%-->
    <Color x:Key="AccentColor3">#990072C6</Color>
    <!--20%-->
    <Color x:Key="AccentColor4">#660072C6</Color>

    <LinearGradientBrush x:Key="BrandingBrush"
                         EndPoint="0.001,0.5"
                         StartPoint="1.002,0.5">
        <GradientStop Color="{DynamicResource HighlightDarkColor}"
                      Offset="0" />
        <GradientStop Color="#FF5FC316"
                      Offset="1" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="AccentColorBrush"
                     Color="{StaticResource AccentColor}" />

    <SolidColorBrush x:Key="AccentSelectedColorBrush"
                     Color="White" />
    
    <!--<Image x:Key="Icon" Source="dynamic.data.blue.ico"/>-->

</ResourceDictionary>